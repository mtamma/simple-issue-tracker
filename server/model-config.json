{
    "_meta": {
        "sources": [
            "loopback/common/models",
            "loopback/server/models",
            "../common/models",
            "./models"
        ],
        "mixins": [
            "loopback/common/mixins",
            "loopback/server/mixins",
            "../common/mixins",
            "./mixins"
        ]
    },
    "User": {
        "dataSource": "mongodDatasource"
    },
    "AccessToken": {
        "dataSource": "mongodDatasource",
        "public": false
    },
    "ACL": {
        "dataSource": "mongodDatasource",
        "public": false
    },
    "RoleMapping": {
        "dataSource": "mongodDatasource",
        "public": false,
        "options": {
            "strictObjectIDCoercion": true
        }
    },
    "Role": {
        "dataSource": "mongodDatasource",
        "public": false
    },
    "Issue": {
        "dataSource": "mongoDatasource",
        "public": true
    },
    "Status": {
        "dataSource": "mongoDatasource",
        "public": true
    }
}